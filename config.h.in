/* Configuration header for mconf */

#ifndef CONFIG_H
#define CONFIG_H

/* Define the curses header to include */
#cmakedefine CURSES_LOC @CURSES_LOC@

/* Package name - only define if not already defined */
#ifndef PACKAGE
#define PACKAGE "@PACKAGE_NAME@"
#endif

/* Locale directory */
#define LOCALEDIR "@LOCALEDIR@"

/* Disable NLS if requested */
#cmakedefine KBUILD_NO_NLS

/* Windows specific - don't redefine _WIN32 as it's built-in */
#ifdef _WIN32
#define WINDOWS_BUILD 1
#endif

#endif /* CONFIG_H */